{
  "workbench.colorTheme": "Catppuccin Macchiato",

  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,    
    "**/node_modules/*/**": true,
    "/usr/local/**": true,
